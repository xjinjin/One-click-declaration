<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_form_create_xml_wizard" model="ir.ui.view">
         <field name="name">上传xml文件</field>
         <field name="model">create.xml.obj.wizard</field>
         <field name="arch" type="xml">
             <form string="Create xml">
                 <sheet>
                    <group>
                        <group>
                            <field name="name" />
                            <field name="description" />
                        </group>
                        <group>
                            <field name="dqbm" />
                            <field name="file" />
                            <div></div>
                            <div style="text-align: right;width:190px">
                                <button name="create_xml_obj" type="object"
                                string="上传" />
                            </div>
                        </group>
                    </group>
                 </sheet>
             </form>
         </field>
    </record>
</odoo>