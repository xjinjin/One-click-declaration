<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="view_tree_status" model="ir.ui.view">
         <field name="name">状态查询</field>
         <field name="model">cic_taxsb.status</field>
         <field name="arch" type="xml">
             <tree>
                 <field name="sbzlbh" />
                 <field name="nsrsbh" />
                 <field name="nsqxdm" />
                 <field name="skssqq" />
                 <field name="skssqz" />
             </tree>
         </field>
    </record>

    <record id="view_form_status" model="ir.ui.view">
         <field name="name">状态查询表单</field>
         <field name="model">cic_taxsb.status</field>
         <field name="arch" type="xml">
             <form string="status">
                 <sheet>
                    <group>
                        <group>
                            <field name="sbzlbh" />
                            <field name="nsrsbh" />
                            <field name="nsqxdm" />
                            <field name="skssqq" />
                            <field name="skssqz" />
                        </group>
                    </group>
                 </sheet>
             </form>
         </field>
    </record>
</odoo>